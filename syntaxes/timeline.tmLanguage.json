{
  "name": "Timeline",
  "scopeName": "source.timeline",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [
        { "include": "#timepoint" }
      ]
    },
    "timepoint": {
      "match": "(?<=^)(?:\\s*)(\\.\\d+|(0|[1-9]\\d*)(\\.\\d*)?)(?=\\s)",
      "name": "constant.numeric.timepoint.timeline"
    }
  }
}
