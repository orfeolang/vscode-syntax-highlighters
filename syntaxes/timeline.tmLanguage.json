{
  "name": "Timeline",
  "scopeName": "source.timeline",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [
        { "include": "#timepoint" }
      ]
    },
    "timepoint": {
      "match": "^\\s*(\\.[0-9]+|(0|[1-9][0-9]*)(\\.[0-9]*)?)(?=\\s)",
      "name": "constant.numeric.timepoint.timeline"
    }
  }
}
